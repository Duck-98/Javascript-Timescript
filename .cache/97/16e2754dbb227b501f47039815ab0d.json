ed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":134,"column":15}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":134,"column":18}},{"source":"src/page/news-feed-view.ts","name":null,"original":{"line":31,"column":0},"generated":{"line":134,"column":30}}],"sources":{"src/page/news-feed-view.ts":"import View from '../core/view';\nimport { NewsFeedApi } from '../core/api';\nimport { NewsStore,NewsFeed } from '../types';\nimport { NEWS_URL } from '../config';\n\nconst template = `\n<div class=\"bg-gray-600 min-h-screen\">\n  <div class=\"bg-white text-xl\">\n    <div class=\"mx-auto px-4\">\n      <div class=\"flex justify-between items-center py-6\">\n        <div class=\"flex justify-start\">\n          <h1 class=\"font-extrabold\">Hacker News</h1>\n        </div>\n        <div class=\"items-center justify-end\">\n          <a href=\"#/page/{{__prev_page__}}\" class=\"text-gray-500\">\n            Previous\n          </a>\n          <a href=\"#/page/{{__next_page__}}\" class=\"text-gray-500 ml-4\">\n            Next\n          </a>\n        </div>\n      </div> \n    </div>\n  </div>\n  <div class=\"p-4 text-2xl text-gray-700\">\n    {{__news_feed__}}        \n  </div>\n</div>\n`;\n\nexport default class NewsFeedView extends View {\n  private api: NewsFeedApi;\n  private store: NewsStore; \n\n  constructor(containerId: string, store: NewsStore) {\n    super(containerId, template);\n\n    this.store = store;\n    this.api = new NewsFeedApi();\n\n    \n  }\n\n  render = async (page: string = '1'): Promise<void> => {\n    this.store.currentPage = Number(page);\n\n    if (!this.store.hasFeeds) {\n      this.store.setFeeds(await this.api.getData());\n    }\n\n    for(let i = (this.store.currentPage - 1) * 10; i < this.store.currentPage * 10; i++) {\n      const { id, title, comments_count, user, points, time_ago, read } = this.store.getFeed(i);\n\n      this.addHtml(`\n        <div class=\"p-6 ${read ? 'bg-red-500' : 'bg-white'} mt-6 rounded-lg shadow-md transition-colors duration-500 hover:bg-green-100\">\n          <div class=\"flex\">\n            <div class=\"flex-auto\">\n              <a href=\"#/show/${id}\">${title}</a>  \n            </div>\n            <div class=\"text-center text-sm\">\n              <div class=\"w-10 text-white bg-green-300 rounded-lg px-0 py-2\">${comments_count}</div>\n            </div>\n          </div>\n          <div class=\"flex mt-3\">\n            <div class=\"grid grid-cols-3 text-sm text-gray-500\">\n              <div><i class=\"fas fa-user mr-1\"></i>${user}</div>\n              <div><i class=\"fas fa-heart mr-1\"></i>${points}</div>\n              <div><i class=\"far fa-clock mr-1\"></i>${time_ago}</div>\n            </div>  \n          </div>\n        </div>    \n      `);\n    }  \n\n    this.setTemplateData('news_feed', this.getHtml());\n    this.setTemplateData('prev_page', String(this.store.prevPage));\n    this.setTemplateData('next_page', String(this.store.nextPage));\n  \n    this.updateView();\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"b2ee0df377c79087b97ef87006ecabc9","cacheData":{"env":{}}}